
#esses comandos abaixo devem ser rodados no terminal e nÃ£o dentro desse script
sudo taskset -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp1; mv output_predictor.txt output_predictor_exp1; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp2; mv output_predictor.txt output_predictor_exp2; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror;  mv stderror stderror_exp3; mv output_predictor.txt output_predictor_exp3; sleep 7; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp4; mv output_predictor.txt output_predictor_exp4;sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp5; mv output_predictor.txt output_predictor_exp5; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp6; mv output_predictor.txt output_predictor_exp6; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror;  mv stderror stderror_exp7; mv output_predictor.txt output_predictor_exp7; sleep 7; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp8; mv output_predictor.txt output_predictor_exp8; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror;mv stderror stderror_exp9; mv output_predictor.txt output_predictor_exp9; sleep 5; sudo taskset  -a -c 0-7 ./bin/scheduler > /dev/null 2> stderror; mv stderror stderror_exp10; mv output_predictor.txt output_predictor_exp10; tar -cf result.tar output_predictor_exp* stderror*
